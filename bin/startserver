"use strict";

var util = require('../lib/util');
var ipv4 = require('../lib/ipv4');
var StartServer = require('../');
var debug = require('debug')('bin/startserver:');
var fs = require('fs');
var path = require('path');
var DataBase = require('datautil');

var server = new StartServer();

util.merge(server, DataBase);

server.bundle();

server.set('woshi',123123);

console.log(server.get('woshi'))

server.on('hao',function(e){
  console.log(e)
})

server.emit('hao','99999999999999')

server.listen(8888, function(){

});
